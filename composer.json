{
  "name": "kennisnet/oaipmh",
  "description": "This package can be used to create an OAI endpoint",
  "homepage": "https://github.com/kennisnet/oaipmh",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Kennisnet\\OaiPmh\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kennisnet\\OaiPmh\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "ext-dom": "*",
    "psr/http-message": "^1.1 || ^2.0",
    "guzzlehttp/psr7": "^1.2 || ^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "laminas/laminas-diactoros": "^1.1 || ^2.0",
    "squizlabs/php_codesniffer": "^2.3 || ^3.7",
    "php-coveralls/php-coveralls": "^1.1 || ^2.5",
    "jameshalsall/licenser": "dev-master"
  }
}
